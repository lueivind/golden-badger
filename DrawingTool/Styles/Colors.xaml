<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">



    <!-- Background -->
    <SolidColorBrush x:Key="background-level-0" Color="{StaticResource blue-grey-darken-4}"></SolidColorBrush>
    <SolidColorBrush x:Key="background-level-1" Color="{StaticResource blue-grey-darken-2}"></SolidColorBrush>
    <SolidColorBrush x:Key="background-level-2" Color="{StaticResource blue-grey-lighten-2}"></SolidColorBrush>

    <SolidColorBrush x:Key="button-background" Color="{StaticResource grey-darken-4}"></SolidColorBrush>
    <SolidColorBrush x:Key="button-border" Color="{StaticResource grey-darken-3}"></SolidColorBrush>


    <!--Foreground-->
    <SolidColorBrush x:Key="foreground-level-0" Color="{StaticResource blue-grey-lighten-5}"></SolidColorBrush>
    <SolidColorBrush x:Key="foreground-level-1" Color="{StaticResource blue-grey-lighten-4}"></SolidColorBrush>


</ResourceDictionary>